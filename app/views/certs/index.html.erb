<h1>Listing certs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>User</th>
      <th>Date</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @certs.each do |cert| %>
      <tr>
        <td><%= cert.title %></td>
        <td><%= cert.user_id %></td>
        <td><%= cert.date %></td>
        <td><%= link_to 'Show', cert %></td>
        <td><%= link_to 'Edit', edit_cert_path(cert) %></td>
        <td><%= link_to 'Destroy', cert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cert', new_cert_path %>
